# Secrets file example (Home Assistant style)
# Copy this to secrets.yaml and fill in your own ICS URLs
# Add secrets.yaml to .gitignore to keep URLs private
#
# Alternative: Use environment variables instead
# Set environment variables with SECRET_ prefix (uppercase):
#   SECRET_WELPEN_ICS_URL="https://..."
#   SECRET_SCOUTS_ICS_URL="https://..."
#   SECRET_GROEPSBREED_PASSWORD="your-password"
#
# Priority: Environment variables take precedence over secrets.yaml

welpen_ics_url: "https://your-calendar-service.com/path/to/welpen.ics"
scouts_ics_url: "https://your-calendar-service.com/path/to/scouts.ics"

# Optional: Password protection for calendars
# groepsbreed_password: "your-secure-password"

# You can add more secrets as needed:
# other_calendar_url: "https://..."
# api_key: "your-api-key"
